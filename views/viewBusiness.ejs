<img src="<%= business.image_logo %>"><br>
Business Name: <%= business.name %> <br>
Address: <%= business.address %> <br>
Phone Number: <%= business.phone_number %> <br>
Category: <%= business.category.name %> <br>
<a href="/review/add/<%=business.id %>">Add review</a><br>
<hr>
<% business.review.forEach(function(review){ %>
    title: <%= review.title%><br>
    description: <%= review.description%><br>
    rating: <%= review.rating%>
<hr>
<% })%>